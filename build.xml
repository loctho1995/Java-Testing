<project name="Baitap1_1" default="default" basedir=".">
    <description>Builds, tests, and runs the project Baitap1_1.</description>
    <import file="nbproject/build-impl.xml"/>

	<target name="-post-compile">
            <obfuscate>
                <fileset dir="${build.classes.dir}"/>
            </obfuscate>
        </target>

        <target name="run" depends="Baitap1_1-impl.jar">
            <exec dir="bin" executable="launcher.exe">
                <arg file="${dist.jar}"/>
            </exec>
        </target>
</project>
